{
  "manifest_version": 3,
  "name": "Canvas Quiz Tracking Blocker",
  "version": "1.0",
  "description": "Blocks canvas event requests during a quiz.",
  "permissions": ["scripting", "storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["injected.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "action": {
    "default_title": "Canvas Quiz Tracking Blocker"
  }
}
